{
	"version": "2.0",
	"columns": 3,
	"tiles": [
		{
			"type": "PING",
			"params": {
				"hostname": "api.zws.im"
			}
		},
		{
			"type": "HTTP-FORMATTED",
			"label": "API Version",
			"params": {
				"url": "http://api:3000/stats?format=true",
				"format": "JSON",
				"key": "version"
			}
		},
		{
			"type": "GITHUB-CHECKS",
			"params": {
				"owner": "zws-im",
				"repository": "zws",
				"ref": "master"
			}
		},
		{
			"type": "HTTP-RAW",
			"label": "GitHub stars",
			"params": {
				"url": "https://github.com/zws-im/zws",
				"regex": "(\\d+) users starred"
			}
		},
		{
			"type": "HTTP-FORMATTED",
			"label": "URLs shortened",
			"params": {
				"url": "http://api:3000/stats?format=true",
				"format": "JSON",
				"key": "urls"
			}
		},
		{
			"type": "HTTP-FORMATTED",
			"label": "URLs visited",
			"params": {
				"url": "http://api:3000/stats?format=true",
				"format": "JSON",
				"key": "visits"
			}
		}
	]
}
